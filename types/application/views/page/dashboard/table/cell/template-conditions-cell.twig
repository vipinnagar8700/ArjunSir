{% if templates|length > 0 %}
<h4 class="toolset-table-list-heading"><span class="icon-views-logo"></span> {{ __( 'Conditional Templates', 'wpcf' )|e('html') }}</h4>
<ul class="toolset-table-list">
	{% for ct_link in templates %}
		<li><a href="{{ ct_link.link }}&ref=dashboard">{{ ct_link.title|e('html') }}</a></li>
	{% endfor %}
</ul>
{% endif %}
